{

  "description": "Blocks websites from port scanning your computer",
  "manifest_version": 2,
  "name": "Port Authority",
  "version": "1.0.4",
  "homepage_url": "https://github.com/ACK-J/Port_Authoirty",
  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  }

}
